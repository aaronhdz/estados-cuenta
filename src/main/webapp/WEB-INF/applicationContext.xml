<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="isiedoctacred" class="org.springframework.jndi.JndiObjectFactoryBean">
        <property name="jndiName" value="jdbc/ISIEDOCTACRED"/>
        <property name="resourceRef" value="true" />
    </bean>
    <bean id="oracle" class="org.springframework.jndi.JndiObjectFactoryBean">
        <property name="jndiName" value="jdbc/Oracle"/>
        <property name="resourceRef" value="true" />
    </bean>
</beans>
